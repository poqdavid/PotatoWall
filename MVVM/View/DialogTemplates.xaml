<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:PotatoWall.MVVM.View"
    xmlns:vm="clr-namespace:PotatoWall.MVVM.ViewModel">

    <DataTemplate DataType="{x:Type vm:SettingsViewModel}">
        <local:SettingsControl />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:AddIPViewModel}">
        <local:AddIPControl />
    </DataTemplate>
</ResourceDictionary>